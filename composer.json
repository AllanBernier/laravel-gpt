{
    "name": "allanbernier/laravel-gpt",
    "description": "A Laravel package for interacting with the ChatGPT API with built-in support for function calling (tools)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "openai",
        "chatgpt",
        "ai",
        "gpt",
        "chat",
        "function-calling",
        "tools",
        "assistant",
        "llm"
    ],
    "authors": [
        {
            "name": "Allan Bernier"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AllanBernier\\LaravelGpt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AllanBernier\\LaravelGpt\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AllanBernier\\LaravelGpt\\LaravelGptServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
